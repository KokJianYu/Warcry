        <arguments>
          <argument>
            <kind>1</kind>
            <string>mass = 1;

maxSpeed = 30;
//friction = maxSpeed/60/4;
grabbedBy = -1;

oldX = x;
oldY = y;

collidedWithBall = false;
ballCollidedWithId = -1;

</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>
if(keyboard_check(vk_right) &amp;&amp; place_free(x+8,y))
{
if(hspeed&lt;10)
    hspeed=10;
}
if(keyboard_check(vk_left) &amp;&amp; place_free(x,y))
{
if(hspeed&gt;-10)
    hspeed=-10;
}
if(keyboard_check(vk_up) &amp;&amp; place_free(x,y-8))
{
if(vspeed&gt;-10)
    vspeed=-10;
}
if(keyboard_check(vk_down) &amp;&amp; place_free(x,y+8))
{
if(vspeed&lt;10)
    vspeed=10;
}


//This reduces speed to the max speed indicated.
if(power(vspeed,2)+power(hspeed,2)&gt;power(maxSpeed,2))
{
    vspeed = vspeed/sqrt(power(vspeed,2)+power(hspeed,2))*maxSpeed;
    hspeed = hspeed/sqrt(power(vspeed,2)+power(hspeed,2))*maxSpeed;
}
    
if(power(vspeed,2)+power(hspeed,2) &lt;= maxSpeed*0.35)
{
    sprite_index = sprite_greenBall;
}
else
    sprite_index = sprite_ball;
    
//Check if the ball is currently colliding with another ball.
// or if it is grabbed. 
if(!collidedWithBall &amp;&amp; grabbedBy == -1)    
    checkForBallCollision();
if((speed&gt;0) &amp;&amp; !collidedWithCharacter)
{
    checkForCharacterCollision();
}

if(!place_meeting(x+hspeed,y+vspeed,obj_player) &amp;&amp; !place_meeting(x+hspeed,y+vspeed,obj_player2) )
{
    collidedWithCharacter = false;
}
    
if(!place_meeting(x+hspeed,y+vspeed,ball))
{
    collidedWithBall = false;
    ballCollidedWithId = -1;
}
 
//This if else statement prevents the ball from being blocked by
//objects when it is grabbed.   
if(grabbedBy != -1)
{

    var lengthRequired =  sprite_width + 6;
    x = grabbedBy.x + cos(degtorad(grabbedBy.direction))*lengthRequired;
    y = grabbedBy.y - sin(degtorad(grabbedBy.direction))*lengthRequired; // y-axis is reversed
    vspeed = 0;
    hspeed = 0;
}
else
{
    move_bounce_solid(true);
    //move_outside_all(radtodeg(arctan2(vspeed,hspeed)),maxSpeed);
}



</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="64">
      <action>
        <libid>1</libid>
