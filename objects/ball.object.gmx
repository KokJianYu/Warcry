        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>draw_text(900,300,radtodeg(arctan2(-vspeed,hspeed)));
</string>
          </argument>
        </arguments>
      </action>
    </event>
