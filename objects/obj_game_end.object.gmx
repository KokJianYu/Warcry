draw_text(room_width/4, room_height/2-100, "Player 1");
draw_text(room_width/2+room_width/4, room_height/2-100, "Player 2");

var player1Outcome;
var player2Outcome;
if(global.player1Lives &gt; global.player2Lives)
{
    player1Outcome = "Win";
    player2Outcome = "Lose";
} 
else if(global.player1Lives &lt; global.player2Lives)
{
    player1Outcome = "Lose"; 
    player2Outcome = "Win";
}
else
{
    if(global.player1TotalDamageTaken &gt; global.player2TotalDamageTaken)
    {   
        player1Outcome = "Lose";
        player2Outcome = "Win";
    }
    else if(global.player1TotalDamageTaken &lt; global.player2TotalDamageTaken)
    {
        player1Outcome = "Win";
        player2Outcome = "Lose";
    }
    else
    {
        player1Outcome = "Draw";
        player2Outcome = "Draw";
    }
    

}
draw_text(room_width/4, room_height/2-50, player1Outcome);
draw_text(room_width/2+room_width/4, room_height/2-50, player2Outcome);

draw_text(room_width/2, room_height/2, "Lives Remaining");
draw_text(room_width/4, room_height/2, global.player1Lives);
